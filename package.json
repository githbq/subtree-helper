{
    "name": "subtree-helper",
    "version": "1.0.0",
    "description": "subtree-helper",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "main": "build/command",
    "bin": {
        "subtree": "./build/bin/index.js"
    },
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run clear && tsc -w",
        "p": "npm run tsc && npm publish",
        "test": "jest --forceExit --detectOpenHandles --colors",
        "lint": "tslint --fix '{src,__tests__}/**/*.ts'"
    },
    "dependencies": {
        "bluebird": "^3.7.2",
        "cli-color": "^2.0.0",
        "fs-extra": "^9.0.1",
        "fs-extra-new": "^1.0.3",
        "inquirer": "^7.3.3",
        "isos": "^0.0.3",
        "json-stringify-pretty-compact": "^2.0.0",
        "lodash": "^4.17.4",
        "moment-helper": "^0.0.2",
        "ora": "^1.2.0",
        "pretty-ms": "^2.1.0",
        "require-dir": "^0.3.1",
        "spawn-helper": "^0.0.5",
        "tslib": "^2.0.0",
        "yargs": "^7.1.0"
    },
    "devDependencies": {
        "@types/cli-color": "^0.3.29",
        "@types/fs-extra": "^9.0.1",
        "@types/jest": "^26.0.13",
        "@types/lodash": "^4.14.64",
        "@types/node": "^6.0.48",
        "jest": "^26.4.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.3.0",
        "ts-node": "^3.0.2",
        "tslint": "^5.1.0",
        "typescript": "^3.9.7"
    },
    "keywords": [
        "typescript"
    ],
    "author": "hubiqing",
